<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:local="clr-namespace:westgateproject" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" Title="상가연합회 주차장" x:Class="westgateproject.View.PageForEachFloor.office.unionParking">
    <ScrollView Orientation="Horizontal" BackgroundColor="Gainsboro" x:Name="sv">
        <AbsoluteLayout  x:Name="absL" >
            <Label Text="&lt;만남의 광장" VerticalTextAlignment="Center" HorizontalTextAlignment="Center" BackgroundColor="Transparent" TextColor="Blue" FontSize="15"
            AbsoluteLayout.LayoutBounds="0, 0, 80, 30">
                <Label.GestureRecognizers>
                    <TapGestureRecognizer
                        Tapped="goBack"
                        NumberOfTapsRequired="1"/>
                </Label.GestureRecognizers>
            </Label>
            <Image x:Name="parkingImage" Source="unionParking.png" Aspect="AspectFill"/>

        </AbsoluteLayout>
    </ScrollView>
</ContentPage>
